{
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "/(.*).css",
            "dest": "/$1.css"
        },
        {
            "src": "/(.*).png",
            "dest": "/$1.png"
        },
        {
            "src": "/(.*).jpg",
            "dest": "/$1.jpg"
        },
        {
            "src": "/(.*).ico",
            "dest": "/$1.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}